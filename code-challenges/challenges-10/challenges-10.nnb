            "source": [
                "function birthdayCakeCandles(candles) {\n    let max = Math.max(...candles);\n    let sum = 0;\n    candles.forEach(candle => {\n      if(candle == max) {\n          sum++;\n      }  \n    })\n    return sum;\n}\nbirthdayCakeCandles([5,6,4,6,4]);"
